+	cd po && $(MAKE) wget.pot
+	mkdir $(DISTNAME)
+	for d in `$(FIND) . -type d ! -regex '.*/\.\(svn\|hg\)\(/.*\)?' -print`; do \
+		if [ "$$d" != "." -a "$$d" != "./$(DISTNAME)" ]; then \
+			mkdir $(DISTNAME)/$$d; \
+		fi; \
+	done
+	for f in `cat DISTFILES`; do \
+		ln $(srcdir)/$$f $(DISTNAME)/$$f || \
+		{ echo copying $$f; cp -p $(srcdir)/$$f $(DISTNAME)/$$f ; } \
+	done
